{"ast":null,"code":"import \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nexport default {\n  data() {\n    return {\n      inputUrl: '',\n      outputUrl: ''\n    };\n  },\n  methods: {\n    convertUrl() {\n      if (!this.inputUrl) return;\n      try {\n        const url = new URL(this.inputUrl);\n        if (!url.hostname.includes('bsky.app') || !url.pathname.includes('/feed_fullsize/')) {\n          throw new Error('无效的BlueSky图片链接');\n        }\n        const path = url.pathname.split('/');\n        const did = path[path.length - 2];\n        const cid = path[path.length - 1].split('@')[0];\n        this.outputUrl = `https://bsky.social/xrpc/com.atproto.sync.getBlob?did=${did}&cid=${cid}`;\n      } catch (error) {\n        alert(error.message || '无效的链接,请检查输入');\n        this.outputUrl = '';\n      }\n    },\n    copyUrl() {\n      navigator.clipboard.writeText(this.outputUrl);\n      alert('链接已复制到剪贴板');\n    },\n    clearAll() {\n      this.inputUrl = '';\n      this.outputUrl = '';\n    }\n  }\n};","map":{"version":3,"names":["data","inputUrl","outputUrl","methods","convertUrl","url","URL","hostname","includes","pathname","Error","path","split","did","length","cid","error","alert","message","copyUrl","navigator","clipboard","writeText","clearAll"],"sources":["D:\\文档\\A 项目\\SkyFetch\\skyfetch\\src\\App.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"content\">\n      <h1>SkyFetch</h1>\n      <div class=\"card\">\n        <div class=\"input-group\">\n          <label>原始链接:</label>\n          <input v-model=\"inputUrl\" placeholder=\"粘贴BlueSky图片链接\" />\n          <button @click=\"convertUrl\">转换</button>\n        </div>\n        <div v-if=\"outputUrl\" class=\"output-group\">\n          <label>转换后链接:</label>\n          <input :value=\"outputUrl\" readonly />\n          <button @click=\"copyUrl\">复制</button>\